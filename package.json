{
  "name": "wuerfelblock",
  "version": "0.1.0",
  "author": "Lukas Panni",
  "homepage": "https://lukaspanni.de/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test --karma-config ./karma.conf.js",
    "test-ci": "ng test --karma-config ./karma-ci.conf.js",
    "lint": "ng lint --fix",
    "lint-ci": "ng lint",
    "e2e": "ng e2e",
    "prepare-deploy": "ng build --configuration production &&  docker build -t wuerfelblock .",
    "local-deploy": "docker run --rm -d -p 8080:80 --name wuerfelblock-container wuerfelblock",
    "server-deploy": "docker save -o wuerfelblock.tar wuerfelblock && pscp ./wuerfelblock.tar Server_Secure:wuerfelblock",
    "android": "ionic cordova run android",
    "android-build": "ionic cordova build android",
    "android-build-release": "ionic cordova build android --release --configuration production",
    "android-live": "ionic cordova run android --livereload --consolelogs",
    "preversion": "npm run lint-ci",
    "version": "cross-var npx cordova-update-config-enhanced --appversion $npm_package_version --androidversion && git add config.xml",
    "postversion": "git push && git push --tags"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^14.2.4",
    "@angular/core": "^14.2.4",
    "@angular/forms": "^14.2.4",
    "@angular/platform-browser": "^14.2.4",
    "@angular/platform-browser-dynamic": "^14.2.4",
    "@angular/router": "^14.2.4",
    "@ionic/angular": "^6.2.9",
    "@ionic/cli": "^6.20.2",
    "@ionic/cordova-builders": "^7.0.0",
    "@ionic/storage-angular": "^3.0.6",
    "rxjs": "~7.5.7",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1402.4",
    "@angular-devkit/build-angular": "^14.2.4",
    "@angular-eslint/builder": "^14.1.2",
    "@angular-eslint/eslint-plugin": "^14.1.2",
    "@angular-eslint/eslint-plugin-template": "^14.1.2",
    "@angular-eslint/template-parser": "^14.1.2",
    "@angular/cli": "^14.2.4",
    "@angular/compiler": "^14.2.4",
    "@angular/compiler-cli": "^14.2.4",
    "@angular/language-service": "^14.2.4",
    "@chiragrupani/karma-chromium-edge-launcher": "^2.2.2",
    "@ionic/angular-toolkit": "^7.0.0",
    "@types/jasmine": "~4.3.0",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^18.8.0",
    "@typescript-eslint/eslint-plugin": "5.38.1",
    "@typescript-eslint/parser": "5.38.1",
    "cordova-android": "^11.0.0",
    "cordova-browser": "^6.0.0",
    "cordova-ios": "^6.2.0",
    "cordova-plugin-device": "2.1.0",
    "cordova-plugin-ionic-keyboard": "^2.2.0",
    "cordova-plugin-ionic-webview": "^5.0.0",
    "cordova-plugin-splashscreen": "6.0.2",
    "cordova-plugin-statusbar": "3.0.0",
    "cordova-plugin-whitelist": "^1.3.5",
    "cordova-update-config-enhanced": "^2.0.1",
    "cross-var": "^1.1.0",
    "eslint": "^8.24.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "39.3.6",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "jasmine-core": "~4.4.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-firefox-launcher": "^2.1.2",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "karma-junit-reporter": "^2.0.1",
    "protractor": "~7.0.0",
    "ts-node": "~10.9.1",
    "typescript": "~4.8.4"
  },
  "description": "An Ionic project",
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {
        "ANDROID_SUPPORT_ANNOTATIONS_VERSION": "27.+"
      },
      "cordova-plugin-ionic-keyboard": {}
    },
    "platforms": [
      "android",
      "browser",
      "ios"
    ]
  }
}
